Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.autohint: 0
Xft.lcdfilter: lcddefault

URxvt*geometry: 100x30
URxvt*iconFile: /usr/share/icons/matefaenza/apps/scalable/terminal.svg

URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*scrollBar_floating: false

URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true

URxvt.perl-ext-common: default,clipboard,url-select

URxvt.keysym.C-c: perl:clipboard:copy
URxvt.keysym.C-v: perl:clipboard:paste

URxvt*internalBorder: 4
URxvt*letterSpace: -2

URxvt*font: xft: DejaVu Sans Mono:size=9:antialias=true:hinting=true
URxvt*boldFont: xft: DejaVu Sans Mono:bold:size=9:antialias=true:hinting=true
URxvt*italicFont: xft: DejaVu Sans Mono:italic:size=9:antialias=true:hinting=true
URxvt*boldItalicFont: xft: DejaVu Sans Mono:bold:italic:size=9:antialias=true:hinting=true

!! Zenburn Colour Scheme

URxvt*background: #3F3F3F
URxvt*foreground: #DCDCCC
URxvt*cursorColor: #DCDCCC
URxvt*cursorColor2: #3F3F3F
URxvt*colorUL: #8CD0D3
URxvt*underlineColor: #F0DFAF
URxvt*colorBD: #E0CF9F
URxvt*colorIT: #656555

!! black dark/light
URxvt*color0: #3F3F3F
URxvt*color8: #5F5F5F
!! red dark/light
